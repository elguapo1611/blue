<% Blue::Box.current.rotated_logs.each do |log| %>
<%= log[:names] %> {

  <% log[:options][:options].each do |o| %>
  <%= o %>
  <% end %>

  <% unless log[:options][:postrotate].nil? %>
  postrotate
    <%= log[:options][:postrotate] %>
  endscript
  <% end %>

  <% unless log[:options][:prerotate].nil? %>
    prerotate
    <%= log[:options][:prerotate] %>
    endscript
  <% end %>
}
<% end %>
